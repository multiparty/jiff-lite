{
  "name": "@jiff/monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "prettier": "prettier -l \"packages/**/src/*.{js,ts,json,md}\"",
    "prettier:fix": "prettier -wl \"packages/**/src/*.{js,ts,json,md}\"",
    "test": "lerna run test",
